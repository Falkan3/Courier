{
    "name": "@falkan3/courier",
    "version": "1.0.0",
    "description": "A virtual assistant which allows clients to interact with a website using preselected topics. Can be used as a websocket based chat client scaffolding.",
    "author": "Adam KociÄ‡ (Falkan3)",
    "homepage": "https://github.com/Falkan3/courier#readme",
    "main": "dist/js/courier.js",
    "module": "dist/js/courier.esm.js",
    "unpkg": "dist/js/courier.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Falkan3/courier.git"
    },
    "publishConfig": { "registry": "https://npm.pkg.github.com/@Falkan3" },
    "bugs": {
        "url": "https://github.com/Falkan3/courier/issues"
    },
    "license": "MIT",
    "keywords": [
        "chat",
        "bot",
        "assistant",
        "plugin"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "test": "jest \"tests/.*/.*\\.test\\.js\"",
        "lint": "eslint {src,tests}/**/*.js",
        "lint:base": "eslint src tests",
        "lint:fix": "eslint {src,tests}/**/*.js --fix",
        "stylelint": "stylelint src/assets/scss",
        "stylelint:fix": "stylelint src/assets/scss --fix",
        "scss:core": "node-sass src/assets/scss/courier.core.scss dist/css/courier.core.css && node-sass src/assets/scss/courier.core.scss dist/css/courier.core.min.css --output-style=compressed",
        "scss:theme": "node-sass src/assets/scss/courier.theme.scss dist/css/courier.theme.css && node-sass src/assets/scss/courier.theme.scss dist/css/courier.theme.min.css --output-style=compressed",
        "build:css": "npm run scss:core && npm run scss:theme",
        "build:esm": "rollup --config build/esm.js && rollup --config build/esm.modular.js",
        "build:umd": "rollup --config build/umd.js && rollup --config build/umd.min.js",
        "build:js": "npm run build:esm && npm run build:umd",
        "build": "npm run build:css && npm run build:js",
        "build--deprecated": "cross-env BABEL_ENV=production babel src --out-dir dist",
        "prepublish": "npm run clean && npm run test && npm run build"
    },
    "watch": {
        "test": "{src,tests}/**/*.js",
        "build:js": "src/**/*.js",
        "build:css": "assets/**/*.scss"
    },
    "files": [
        "src",
        "dist"
    ],
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/plugin-external-helpers": "^7.12.1",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-transform-flow-strip-types": "^7.12.1",
        "@babel/plugin-transform-object-assign": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@babel/register": "^7.12.1",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^26.6.3",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-minify": "^0.5.0",
        "chai": "^4.1.2",
        "cross-env": "^5.1.3",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "jest": "^26.6.3",
        "jsdom": "11.11.0",
        "node-sass": "^4.14.1",
        "npm-watch": "^0.2.0",
        "nyc": "^13.3.0",
        "reefjs": "^7.6.1",
        "rimraf": "^2.6.2",
        "rollup": "^1.15.3",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-uglify": "^6.0.4",
        "stylelint": "^8.4.0",
        "stylelint-config-standard": "^17.0.0",
        "stylelint-scss": "^2.5.0"
    },
    "dependencies": {},
    "abandonedDependencies": {
        "xstream": "^11.12.0",
        "@cycle/dom": "^22.7.0",
        "@cycle/run": "^5.4.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "yargs": "^16.1.1"
    },
    "directories": {
        "test": "tests"
    }
}
