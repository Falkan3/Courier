{
    "name": "@falkan3/courier",
    "version": "1.5.0",
    "description": "A flexible, easy to use and configurable chat foundation. Can be used as a virtual assistant which allows clients to interact with a website using preselected topics, or as a websocket based chat scaffolding.",
    "author": "Adam KociÄ‡ (Falkan3)",
    "homepage": "https://falkan3.github.io/Courier/",
    "type": "module",
    "main": "dist/js/courier.js",
    "module": "dist/js/esm/modular/courier.esm.js",
    "unpkg": "dist/js/courier.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Falkan3/courier.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/@Falkan3"
    },
    "bugs": {
        "url": "https://github.com/Falkan3/courier/issues"
    },
    "license": "CC BY-NC-ND 4.0 DEED",
    "keywords": [
        "chat",
        "bot",
        "assistant",
        "plugin"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "test": "jest \"tests/.*/.*\\.test\\.js\" --config jest.config.cjs",
        "lint": "eslint {src,tests}/**/*.js",
        "lint:base": "eslint src tests",
        "lint:fix": "eslint {src,tests}/**/*.js --fix",
        "stylelint": "stylelint src/assets/scss",
        "stylelint:fix": "stylelint src/assets/scss --fix",
        "scss:core": "sass src/assets/scss/courier.core.scss dist/css/courier.core.css && sass src/assets/scss/courier.core.scss dist/css/courier.core.min.css --style=compressed",
        "scss:theme": "npm run scss:theme:base && npm run scss:theme:widget-message-bubble",
        "scss:theme:base": "sass src/assets/scss/courier.theme.scss dist/css/courier.theme.css && sass src/assets/scss/courier.theme.scss dist/css/courier.theme.min.css --style=compressed",
        "scss:theme:widget-message-bubble": "sass src/assets/scss/widget-message-bubble.theme.scss dist/css/widget-message-bubble.theme.css && sass src/assets/scss/widget-message-bubble.theme.scss dist/css/widget-message-bubble.theme.min.css --style=compressed",
        "scss:modules": "npm run scss:chat && npm run scss:popup",
        "scss:chat": "sass src/assets/scss/modules/courier.chat.scss dist/css/modules/courier.chat.css && sass src/assets/scss/modules/courier.chat.scss dist/css/modules/courier.chat.min.css --style=compressed",
        "scss:popup": "sass src/assets/scss/modules/courier.popup.scss dist/css/modules/courier.popup.css && sass src/assets/scss/modules/courier.popup.scss dist/css/modules/courier.popup.min.css --style=compressed",
        "build:css": "npm run scss:core && npm run scss:theme && npm run scss:modules",
        "build:esm": "rollup --config build/esm.js && rollup --config build/esm.modular.js",
        "build:esm:modules": "npm run build:esm:chat && npm run build:esm:popup",
        "build:esm:chat": "rollup --config build/modules/esm.chat.js",
        "build:esm:popup": "rollup --config build/modules/esm.popup.js",
        "build:umd": "rollup --config build/umd.js && rollup --config build/umd.min.js",
        "build:umd:modules": "npm run build:umd:chat && npm run build:umd:popup",
        "build:umd:chat": "rollup --config build/modules/umd.chat.js && rollup --config build/modules/umd.chat.min.js",
        "build:umd:popup": "rollup --config build/modules/umd.popup.js && rollup --config build/modules/umd.popup.min.js",
        "build:js": "npm run build:esm && npm run build:esm:modules && npm run build:umd && npm run build:umd:modules",
        "build:js:modules": "npm run build:esm:modules && npm run build:umd:modules",
        "build": "npm run build:css && npm run build:js",
        "build--deprecated": "cross-env BABEL_ENV=production babel src --out-dir dist",
        "prepare": "npm run clean && npm run test && npm run build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    },
    "watch": {
        "test": "{src,tests}/**/*.js",
        "build:js": "src/**/*.js",
        "build:css": "assets/**/*.scss"
    },
    "files": [
        "src",
        "dist"
    ],
    "engines": {
        "node": ">=18.14.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.23.9",
        "@babel/eslint-parser": "^7.23.10",
        "@babel/plugin-external-helpers": "^7.23.3",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-syntax-import-assertions": "^7.23.3",
        "@babel/plugin-transform-flow-strip-types": "^7.23.3",
        "@babel/plugin-transform-object-assign": "^7.23.3",
        "@babel/plugin-transform-runtime": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@babel/register": "^7.23.7",
        "@babel/runtime-corejs3": "^7.23.9",
        "@glidejs/glide": "^3.6.0",
        "@lopatnov/rollup-plugin-uglify": "^2.1.5",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-url": "^8.0.2",
        "babel-jest": "^29.7.0",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-module-resolver": "^5.0.0",
        "babel-preset-minify": "^0.5.2",
        "chai": "^5.1.0",
        "compress-tag": "^3.0.0",
        "core-js": "^3.36.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.33.2",
        "gh-pages": "^6.1.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^24.0.0",
        "npm-watch": "^0.11.0",
        "nyc": "^15.1.0",
        "reefjs": "13.0.2",
        "rimraf": "^5.0.5",
        "rollup": "^4.12.0",
        "sass": "^1.71.1",
        "stylelint": "^16.2.1",
        "stylelint-config-standard": "^36.0.0",
        "stylelint-scss": "^6.1.0",
        "terser": "^5.28.1",
        "throttle-debounce": "^5.0.0",
        "vue": "^3.4.26"
    },
    "abandonedDependencies": {
        "xstream": "^11.12.0",
        "@cycle/dom": "^22.7.0",
        "@cycle/run": "^5.4.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "yargs": "^16.1.1",
        "@babel/plugin-transform-runtime": "^7.12.1"
    },
    "directories": {
        "test": "tests"
    }
}
